{
  "name": "mini-auth",
  "version": "0.1.0",
  "description": "auth with jwt using cookies",
  "main": "./dist/server.js",
  "scripts": {
    "test": "jest",
    "test:dev": "jest --coverage --watchAll",
    "lint": "eslint . --ext .js",
    "prettier": "prettier --write .",
    "start": "NODE_PATH=./dist node dist/server.js",
    "build": "tsc -p .",
    "dev": "nodemon -L server.ts",
    "compose:dev:up": "docker-compose -f docker-compose.dev.yml up",
    "compose:dev:test": "docker-compose -f docker-compose.dev.yml up mongodb",
    "compose:dev:down": "docker-compose -f docker-compose.dev.yml down -v",
    "compose:prod:up": "docker-compose -f docker-compose.prod.yml up -d --build",
    "compose:prod:down": "docker-compose -f docker-compose.prod.yml down"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/turkaytunc/node-mini-auth.git"
  },
  "keywords": [
    "auth",
    "jwt",
    "cookie",
    "nodejs"
  ],
  "author": "Türkay Tunç",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/turkaytunc/node-mini-auth/issues"
  },
  "homepage": "https://github.com/turkaytunc/node-mini-auth#readme",
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.13.15",
    "@types/bcrypt": "^3.0.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/morgan": "^1.9.2",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@jest/globals": "^26.6.2",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.4",
    "morgan": "^1.10.0",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3"
  }
}
